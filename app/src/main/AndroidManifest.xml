<?xml version="1.0" encoding="utf-8"?>
<manifest package="io.github.yuweiguocn.demo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ShapeDrawableActivity"
            android:label="@string/activity_shape_drawable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEMO"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".about.AboutActivity"
            android:label="@string/activity_about"/>
        <activity
            android:name=".transition.TransitionActivity"
            android:label="@string/activity_transition">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEMO"/>
            </intent-filter>
        </activity>
        <activity android:name=".transition.TransitionResultActivity"
            android:label="@string/activity_transition_result"
            >
        </activity>
    </application>

</manifest>